{
  "name": "ngaren.com",
  "version": "1.0.0",
  "description": "Official website of Ngaren",
  "main": "index.js",
  "scripts": {
    "build": "jalla build index.js --sw sw.js",
    "start": "node server.js",
    "purge": "node -r dotenv/config -e \"require('./lib/purge')(['/sw.js'], console.log)\"",
    "deploy": "npx now --scope codeandconspire && npx now alias --scope codeandconspire",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeandconspire/ngaren.com.git"
  },
  "documentify": {
    "transform": [
      [
        "./lib/document.js",
        {
          "order": "end"
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "now": {
    "alias": "ngaren.conspire.dev",
    "env": {
      "NODE_ENV": "production",
      "CLOUDINARY_KEY": "@cloudinary-key",
      "CLOUDINARY_SECRET": "@cloudinary-secret",
      "PRISMIC_SECRET": "@prismic-ngaren-secret",
      "CLOUDFLARE_ZONE": "@cloudflare-ngaren-zone",
      "CLOUDFLARE_EMAIL": "@cloudflare-ngaren-email",
      "CLOUDFLARE_KEY": "@cloudflare-ngaren-key",
      "POLYFILL_FEATURES": "default-3.3,Array.prototype.find,Array.prototype.findIndex,Array.prototype.includes"
    }
  },
  "author": "code and conspire",
  "license": "CC-BY-SA-4.0",
  "bugs": {
    "url": "https://github.com/codeandconspire/ngaren.se/issues"
  },
  "homepage": "https://github.com/codeandconspire/ngaren.se#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "choo-devtools": "^3.0.0",
    "standard": "^10.0.3"
  },
  "dependencies": {
    "can-autoplay": "^3.0.0",
    "cccpurge": "^2.0.0",
    "choo": "^7.0.0",
    "choo-meta": "^2.0.0",
    "choo-service-worker": "^3.0.0",
    "cloudinary": "^1.13.2",
    "dotenv": "^6.2.0",
    "jalla": "^1.0.0-13",
    "koa-body": "^4.0.8",
    "koa-compose": "^4.1.0",
    "koa-route": "^3.2.0",
    "nanolru": "^1.0.0",
    "normalize.css": "^8.0.1",
    "on-intersect": "^2.0.0",
    "postcss-custom-properties": "^8.0.9",
    "postcss-url": "^8.0.0",
    "prismic-element": "^2.0.2",
    "prismic-helpers": "^1.0.0",
    "prismic-javascript": "^2.0.1",
    "prismic-richtext": "^0.4.2"
  }
}
